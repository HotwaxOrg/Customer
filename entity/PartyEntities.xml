<?xml version="1.0" encoding="UTF-8"?>
<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchema="http://moqui.org/xsd/entity-definition-3.xsd">

    <entity entity-name="Party" package="customer.party" short-alias="parties">
        <field name="partyId" type="id" is-pk="true"/>
        <field name="partyTypeEnumId" type="id"/>

        <relationship type="one" related="moqui.basic.Enumeration" title="PartyType" short-alias="type">
            <key-map field-name="partyTypeEnumId"/>
        </relationship>
        <relationship type="one-nofk" related="customer.person.Person" short-alias="person" mutable="true"/>
        <relationship type="one-nofk" related="customer.organization.Organization" short-alias="organization"
                      mutable="true"/>
        <relationship type="many" related="customer.partyrole.PartyRole" short-alias="roles">
            <key-map field-name="partyId"/>
        </relationship>

        <seed-data>
            <moqui.basic.EnumerationType description="Party Type" enumTypeId="PartyType"/>
            <moqui.basic.Enumeration description="Person" enumId="PtyPerson" enumTypeId="PartyType"/>
            <moqui.basic.Enumeration description="Organization" enumId="PtyOrganization" enumTypeId="PartyType"/>

            <customer.party.Party partyId="PER123" partyTypeEnumId="PtyPerson"/>
            <customer.party.Party partyId="ORG456" partyTypeEnumId="PtyOrganization"/>
            <customer.party.Party partyId="ORG001" partyTypeEnumId="PtyOrganization"/>
            <customer.party.Party partyId="ORG002" partyTypeEnumId="PtyOrganization"/>
            <customer.party.Party partyId="ORG003" partyTypeEnumId="PtyOrganization"/>
            <customer.party.Party partyId="ORG004" partyTypeEnumId="PtyOrganization"/>
            <customer.party.Party partyId="PER005" partyTypeEnumId="PtyPerson"/>
            <customer.party.Party partyId="PER006" partyTypeEnumId="PtyPerson"/>
            <customer.party.Party partyId="PER007" partyTypeEnumId="PtyPerson"/>
            <customer.party.Party partyId="PER008" partyTypeEnumId="PtyPerson"/>
            <customer.party.Party partyId="PER009" partyTypeEnumId="PtyPerson"/>
            <customer.party.Party partyId="PER010" partyTypeEnumId="PtyPerson"/>
        </seed-data>

        <master name="default">
            <detail relationship="type"/>
            <detail relationship="person"/>
            <detail relationship="organization"/>
            <detail relationship="roles"/>
        </master>
     </entity>

    <entity entity-name="Person" package="customer.person" short-alias="person">
        <field name="partyId" type="id" is-pk="true"/>
        <field name="firstName" type="text-short"/>
        <field name="lastName" type="text-short"/>
        <field name="birthDate" type="date"/>

        <relationship type="one" related="customer.party.Party"/>

        <seed-data>
            <customer.person.Person partyId="PER123" firstName="John" lastName="Doe" birthDate="1990-01-01"/>
            <customer.person.Person partyId="PER005" firstName="Viresh" lastName="Shah" birthDate="2004-09-17"/>
            <customer.person.Person partyId="PER006" firstName="Virat" lastName="Kohli" birthDate="1990-10-01"/>
            <customer.person.Person partyId="PER007" firstName="Rohit" lastName="Sharma" birthDate="1980-10-31"/>
            <customer.person.Person partyId="PER008" firstName="Hardik" lastName="Pandya" birthDate="1995-05-05"/>
            <customer.person.Person partyId="PER009" firstName="John" lastName="Cena" birthDate="1990-10-01"/>
            <customer.person.Person partyId="PER010" firstName="Randy" lastName="Doe" birthDate="1992-01-01"/>
        </seed-data>
    </entity>

    <entity entity-name="Organization" package="customer.organization">
        <field name="partyId" type="id" is-pk="true"/>
        <field name="organizationName" type="text-short"/>

        <relationship type="one" related="customer.party.Party"/>

        <seed-data>
            <customer.organization.Organization partyId="ORG456" organizationName="ABC"/>
            <customer.organization.Organization partyId="ORG001" organizationName="DEF"/>
            <customer.organization.Organization partyId="ORG002" organizationName="GHI"/>
            <customer.organization.Organization partyId="ORG003" organizationName="JKL"/>
            <customer.organization.Organization partyId="ORG004" organizationName="MNO"/>
        </seed-data>
    </entity>

    <entity entity-name="PartyRole" package="customer.partyrole">
        <field name="partyId" type="id" is-pk="true"/>
        <field name="roleTypeId" type="id" is-pk="true"/>

        <relationship type="one" related="customer.party.Party" short-alias="party"/>
        <relationship type="one" related="customer.partyroletype.RoleType" short-alias="role"/>

        <seed-data>
            <customer.partyrole.PartyRole partyId="PER123" roleTypeId="EMPLOYEE"/>
            <customer.partyrole.PartyRole partyId="PER123" roleTypeId="CUSTOMER"/>
            <customer.partyrole.PartyRole partyId="ORG456" roleTypeId="EMPLOYER"/>
            <customer.partyrole.PartyRole partyId="ORG001" roleTypeId="EMPLOYER"/>
            <customer.partyrole.PartyRole partyId="ORG002" roleTypeId="CUSTOMER"/>
            <customer.partyrole.PartyRole partyId="ORG003" roleTypeId="EMPLOYER"/>
            <customer.partyrole.PartyRole partyId="ORG004" roleTypeId="CUSTOMER"/>
            <customer.partyrole.PartyRole partyId="PER005" roleTypeId="EMPLOYEE"/>
            <customer.partyrole.PartyRole partyId="PER006" roleTypeId="CUSTOMER"/>
            <customer.partyrole.PartyRole partyId="PER007" roleTypeId="CUSTOMER"/>
            <customer.partyrole.PartyRole partyId="PER008" roleTypeId="EMPLOYEE"/>
            <customer.partyrole.PartyRole partyId="PER009" roleTypeId="CUSTOMER"/>
            <customer.partyrole.PartyRole partyId="PER010" roleTypeId="EMPLOYEE"/>
            <customer.partyrole.PartyRole partyId="PER009" roleTypeId="EMPLOYEE"/>
            <customer.partyrole.PartyRole partyId="PER010" roleTypeId="CUSTOMER"/>
        </seed-data>
    </entity>

    <entity entity-name="RoleType" package="customer.partyroletype">
        <field name="roleTypeId" type="id" is-pk="true"/>
        <field name="description" type="text-medium"/>
        <seed-data>
            <customer.partyroletype.RoleType roleTypeId="CUSTOMER" description="This is a customer"/>
            <customer.partyroletype.RoleType roleTypeId="EMPLOYEE" description="This is an employee"/>
            <customer.partyroletype.RoleType roleTypeId="EMPLOYER" description="This is an employer"/>
        </seed-data>
    </entity>

    <entity entity-name="ContactMech" package="customer.contactmech">
        <field name="contactMechId" type="id" is-pk="true"/>
        <field name="contactMechTypeEnumId" type="id"/>
        <field name="infoString" type="text-medium"/>

        <relationship type="one" related="moqui.basic.Enumeration" short-alias="type"
                      title="contactMechType">
            <key-map field-name="contactMechTypeEnumId"/>
        </relationship>
        <relationship type="one-nofk" related="customer.telecomnumber.TelecomNumber" short-alias="telecomnumber"/>
        <relationship type="one-nofk" related="customer.postaladdress.PostalAddress" short-alias="postaladdress"/>
        <relationship type="many" related="customer.partycontactmech.PartyContactMech"
                      short-alias="parties">
            <key-map field-name="contactMechId"/>
        </relationship>

        <seed-data>
            <moqui.basic.EnumerationType description="Contact Mech Type" enumTypeId="contactMechType"/>
            <moqui.basic.Enumeration description="Telecom Number" enumId="ctyTelecomNumber"
                                     enumTypeId="ContactMechType"/>
            <moqui.basic.Enumeration description="Postal Address" enumId="ctyPostalAddress"
                                     enumTypeId="ContactMechType"/>
            <moqui.basic.Enumeration description="Email Address" enumId="ctyEmailAddress"
                                     enumTypeId="ContactMechType"/>

        </seed-data>

        <master>
            <detail relationship="type"/>
            <detail relationship="telecomnumber"/>
            <detail relationship="postaladdress"/>
        </master>
    </entity>

    <entity entity-name="TelecomNumber" package="customer.telecomnumber">
        <field name="contactMechId" type="id" is-pk="true"/>
        <field name="countryCode" type="text-short"/>
        <field name="areaCode" type="text-short"/>
        <field name="contactNumber" type="text-short"/>

        <relationship type="one" related="customer.contactmech.ContactMech"/>
    </entity>

    <entity entity-name="PostalAddress" package="customer.postaladdress">
        <field name="contactMechId" type="id" is-pk="true"/>
        <field name="toName" type="text-short"/>
        <field name="attnName" type="text-short"/>
        <field name="address1" type="text-medium"/>
        <field name="address2" type="text-medium"/>
        <field name="city" type="text-short"/>
        <field name="postalCode" type="text-short"/>

        <relationship type="one" related="customer.contactmech.ContactMech"/>
    </entity>

    <entity entity-name="ContactMechPurpose" package="customer.contactmechpurpose">
        <field name="contactMechPurposeId" type="id" is-pk="true"/>
        <field name="contactMechTypeEnumId" type="id"/>
        <field name="description" type="text-medium"/>

        <relationship type="one" related="moqui.basic.Enumeration" title="contactMechType">
            <key-map field-name="contactMechTypeEnumId"/>
        </relationship>
    </entity>

    <entity entity-name="PartyContactMech" package="customer.partycontactmech">
        <field name="partyId" type="id" is-pk="true"/>
        <field name="contactMechId" type="id" is-pk="true"/>
        <field name="contactMechPurposeId" type="id" is-pk="true"/>

        <relationship type="one" related="customer.party.Party"/>
        <relationship type="one" related="customer.contactmech.ContactMech" short-alias="contactmech"/>
        <relationship type="one" related="customer.contactmechpurpose.ContactMechPurpose" short-alias="purpose"/>
    </entity>
</entities>